<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AcceptanceSuite">
    <test name="Acceptance Tests">
        <groups>
            <run>
                <include name="acceptance" />
            </run>
        </groups>

        <packages>
            <package name="testcases.*" />
        </packages>
    </test>
</suite>
